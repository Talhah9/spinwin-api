datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
  id              String   @id @default(cuid())
  name            String
  email           String
  paymentProvider String
  paymentId       String
  amountCents     Int
  status          String
  createdAt       DateTime @default(now())
}
